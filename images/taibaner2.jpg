<script type="text/javascript">
    (runinit = window.runinit || []).push(function() {
        $(function() {
            var c_page = 0;
            var totalPage = Math.ceil($('.block-video .detail .v-list li').length / 4);

            $('.block-video .title .bt').removeClass('disabled');
            if (c_page == 0) {
                $('.block-video .title .scr-prev').addClass('disabled');
            }
            if (c_page == totalPage - 1) {
                $('.block-video .title .scr-next').addClass('disabled');
            }

            $('.block-video .title').on('click', '.bt', function(e) {

                var el = $(this);

                if (el.hasClass('disabled')) {
                    return;
                }

                var val = el.hasClass('scr-next') ? 1 : -1;

                var nextP = c_page + val;

                c_page = nextP;

                var pos = $('.block-video .detail .video-list .v-list li:nth-child(' + (c_page * 4 + 1) + ')').position().top;
                $('.block-video .detail .video-list .v-list ul').animate({
                    top: -pos
                }, 250);

                $('.block-video .title .bt').removeClass('disabled');
                if (c_page == 0) {
                    $('.block-video .title .scr-prev').addClass('disabled');
                }
                if (c_page == totalPage - 1) {
                    $('.block-video .title .scr-next').addClass('disabled');
                }
            });

            $('.block-video .detail .video-list .v-list').on('click', 'li', function(e) {
                e.preventDefault();

                init_sport_video(false, this, function () {
                    videoHD.init('.block-video .detail .preview', {
                        type: videoHD.videoType.boxVideo,
                        useBg: false
                    });
                    videoInContent.init('.block-video .detail .preview');
                });
            });

            $('.block-video .detail .video-list .v-list').on('click', 'a', function(e) {
                e.preventDefault();
            });

            init_sport_video(true, $('.block-video .detail .video-list .v-list li:first-child'), function () {
                videoHD.init('.block-video .detail .preview', {
                    type: videoHD.videoType.boxVideo,
                    useBg: false
                });
                videoInContent.init('.block-video .detail .preview');
            });

            
        });
    });
</script>

